.aniSmall {
  -webkit-animation: aniSmall .6s both;
  animation: aniSmall .6s both;
}

.tiem {
  position: absolute;
  top: 2vh;
  z-index: 10000;
  color: #ffffff;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100vw;
}

@-webkit-keyframes aniSmall {
  0% {
    -webkit-transform: scale(1);
    transform: scale(1);
  }

  95% {
    -webkit-transform: scale(0.8);
    transform: scale(0.8);
  }
}

@keyframes aniSmall {
  0% {
    -webkit-transform: scale(1);
    transform: scale(1);
  }

  95% {
    -webkit-transform: scale(0.8);
    transform: scale(0.8);
  }
}

.aniBig {
  -webkit-animation: aniBig .6s both;
  animation: aniBig .6s both;
}

@-webkit-keyframes aniBig {
  0% {
    -webkit-transform: scale(0.8);
    transform: scale(0.8);
  }

  99% {
    -webkit-transform: scale(1);
    transform: scale(1);
  }

  100% {
    -webkit-transform: scale(1);
    transform: scale(1);
  }
}

@keyframes aniBig {
  0% {
    -webkit-transform: scale(0.8);
    transform: scale(0.8);
  }

  99% {
    -webkit-transform: scale(1);
    transform: scale(1);
  }

  100% {
    -webkit-transform: scale(1);
    transform: scale(1);
  }
}

.aniBack {
  -webkit-animation: aniBack .6s both;
  animation: aniBack .6s both;
}

@-webkit-keyframes aniBack {
  0% {
    z-index: 999;
    -webkit-transform: translate(-120%, 50%) scale(0.8) rotate(-30deg);
    transform: translate(-120%, 50%) scale(0.8) rotate(-30deg);
  }

  95% {
    z-index: 999;
    -webkit-transform: translate(0, 0) scale(1) rotate(0);
    transform: translate(0, 0) scale(1) rotate(0);
  }

  96% {
    z-index: 999;
    -webkit-transform: translate(0, 0) scale(1) rotate(0);
    transform: translate(0, 0) scale(1) rotate(0);
  }

  100% {
    z-index: 999;
    -webkit-transform: translate(0, 0) scale(1) rotate(0);
    transform: translate(0, 0) scale(1) rotate(0);
  }
}

@keyframes aniBack {
  0% {
    z-index: 999;
    -webkit-transform: translate(-120%, 50%) scale(0.8) rotate(-30deg);
    transform: translate(-120%, 50%) scale(0.8) rotate(-30deg);
  }

  95% {
    z-index: 999;
    -webkit-transform: translate(0, 0) scale(1) rotate(0);
    transform: translate(0, 0) scale(1) rotate(0);
  }

  96% {
    z-index: 999;
    -webkit-transform: translate(0, 0) scale(1) rotate(0);
    transform: translate(0, 0) scale(1) rotate(0);
  }

  100% {
    z-index: 999;
    -webkit-transform: translate(0, 0) scale(1) rotate(0);
    transform: translate(0, 0) scale(1) rotate(0);
  }
}

.aniNext {
  -webkit-animation: aniNext .6s both;
  animation: aniNext .6s both;
}

@-webkit-keyframes aniNext {
  0% {
    z-index: 999;
    -webkit-transform: translate(0, 0) scale(1) rotate(0);
    transform: translate(0, 0) scale(1) rotate(0);
  }

  95% {
    z-index: 999;
    -webkit-transform: translate(-120%, 50%) scale(0.8) rotate(-30deg);
    transform: translate(-120%, 50%) scale(0.8) rotate(-30deg);
  }

  100% {
    z-index: 999;
    -webkit-transform: translate(-120%, 50%) scale(0.8) rotate(-30deg);
    transform: translate(-120%, 50%) scale(0.8) rotate(-30deg);
  }
}

@keyframes aniNext {
  0% {
    z-index: 999;
    -webkit-transform: translate(0, 0) scale(1) rotate(0);
    transform: translate(0, 0) scale(1) rotate(0);
  }

  95% {
    z-index: 999;
    -webkit-transform: translate(-120%, 50%) scale(0.8) rotate(-30deg);
    transform: translate(-120%, 50%) scale(0.8) rotate(-30deg);
  }

  100% {
    z-index: 999;
    -webkit-transform: translate(-120%, 50%) scale(0.8) rotate(-30deg);
    transform: translate(-120%, 50%) scale(0.8) rotate(-30deg);
  }
}

.back {
  left: 78rpx;
}

.next {
  right: 78rpx;
}

.btn {
  position: absolute;
  bottom: 45rpx;
  width: 100rpx;
  height: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  font-size: 21rpx;
  font-weight: bold;
  color: #0998dc;
}

.container999 {
  position: fixed;
  width: 100vw;
  height: 100vh;
  left: 0;
  top: 0;
}

.container999 .bg {
  position: absolute;
  z-index: 9;
  top: 0;
  left: 0;
  width: 100vw;
  height: 16vh;
  background-color: var(--indexL);
}

.container999 .card {
  width: 90vw;
  position: absolute;
  top: 8vh;
  left: 0;
  right: 0;
  margin: auto;
  z-index: 10;
  min-height: 82vh;
  -webkit-transition: .6s;
  transition: .6s;
  overflow: hidden;
  -webkit-transform: scale(0.8);
  transform: scale(0.8);
  box-shadow: 0rpx 0rpx 17rpx 0rpx rgba(35, 36, 37, 0.25);
  background: #fefefe;
  border-radius: 10rpx;
}

.container999 .card .cardBottom {
  padding-top: 20rpx;
}

.container999 .card .cardBottom .line {
  padding-top: 60rpx;
  width: 87%;
  position: relative;
  margin: auto;
}

.container999 .card .cardBottom .line .lineBottom {
  font-size: 20rpx;
  color: #727272;
  padding-left: 72rpx;
}

.container999 .card .cardBottom .line .lineTop {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
  justify-content: flex-start;
}

.container999 .card .cardBottom .line .lineTop .title {
  padding-left: 30rpx;
}

.container999 .card .cardBottom .line .lineTop .box {
  width: 31rpx;
  height: 31rpx;
  border: 1rpx solid #0998dc;
  border-radius: 2rpx;
}

.container999 .card .cardTop {
  padding-left: 60rpx;
  font-size: 29rpx;
  font-family: 'Microsoft YaHei';
  font-weight: bold;
  height: 14vh;
  background: -webkit-linear-gradient(top, #fefefe 0%, #f9d5bc);
  background: linear-gradient(to bottom, #fefefe 0%, #f9d5bc);
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
  justify-content: flex-start;
}

switch.red[checked] .wx-switch-input,
checkbox.red[checked] .wx-checkbox-input,
radio.red[checked] .wx-radio-input {
  border-color: var(--indexL) !important;
}

switch.red[checked] .wx-switch-input.wx-switch-input-checked,
checkbox.red[checked] .wx-checkbox-input,
radio.red[checked] .wx-radio-input {
  background-color: var(--indexL) !important;
  color: var(--white) !important;
}