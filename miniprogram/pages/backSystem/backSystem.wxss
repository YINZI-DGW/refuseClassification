page {
    background-color: #ffffff;
    width: 100%vw;
}

.background {
    width: 100%;
    height: 400rpx;
    z-index: 11;
}

.outlinedA {
    color: hsl(200, 75%, 35%);
    text-shadow: 0 0 1px currentColor,
        -1px -1px 1px hsl(200, 75%, 45%),
        0 -1px 1px hsl(200, 75%, 40%),
        -1px 0 1px hsl(200, 75%, 35%),
        1px -1px 1px hsl(200, 75%, 35%),
        1px 0 1px hsl(200, 75%, 30%),
        1px 1px 1px hsl(200, 75%, 30%),
        0 1px 1px hsl(200, 75%, 30%),
        -1px 1px 1px hsl(200, 75%, 30%),

        -2px -2px 1px hsl(200, 75%, 75%),
        -2px -1px 1px hsl(200, 75%, 60%),
        -2px 0 1px hsl(200, 75%, 60%),
        -1px -2px 1px hsl(200, 75%, 50%),
        0 -2px 1px hsl(200, 75%, 50%),
        1px -2px 1px hsl(200, 75%, 50%),
        2px -2px 1px hsl(200, 75%, 35%),
        2px -1px 1px hsl(200, 75%, 35%),
        2px 0 1px hsl(200, 75%, 30%),
        2px 1px 1px hsl(200, 75%, 10%),
        2px 2px 1px hsl(200, 75%, 10%),
        1px 2px 1px hsl(200, 75%, 15%),
        0 2px 1px hsl(200, 75%, 10%),
        -1px 2px 1px hsl(200, 75%, 20%),
        -2px 2px 1px hsl(200, 75%, 20%),
        -2px 1px 1px hsl(200, 75%, 10%),

        -3px -3px 1px #006,
        -2px -3px 1px #006,
        -1px -3px 1px #006,
        0 -3px 1px #006,
        1px -3px 1px #006,
        2px -3px 1px #006,
        3px -3px 1px #006,
        3px -2px 1px #006,
        3px -1px 1px #006,
        3px 0 1px #006,
        3px 1px 1px #006,
        3px 2px 1px #006,
        3px 3px 2px #006,
        2px 3px 1px #006,
        1px 3px 1px #006,
        0 3px 1px #006,
        -1px 3px 1px #006,
        -2px 3px 1px #006,
        -3px 3px 1px #006,
        -3px 2px 1px #006,
        -3px 1px 1px #006,
        -3px 0 1px #006,
        -3px -1px 1px #006,
        -3px -2px 1px #006;
    font-size: 68rpx;
}
.login{
    margin-top: 100rpx;
    width: 100%;
}
.account {
  display: flex;
  width: 80vw;
  height: 80rpx;
  border: 2rpx solid rgb(151, 151, 151) ;
  margin: 0 auto;
  font-size: 36rpx;
  line-height: 80rpx;
  padding-left: 50rpx;
  border-radius: 40rpx;
}
input{
    margin-left: 30rpx;
    height: 100%;
}
.password{
    display: flex;
    width: 80vw;
    height: 80rpx;
    border: 2rpx solid rgb(151, 151, 151) ;
    margin: 60rpx auto;
    font-size: 36rpx;
    line-height: 80rpx;
    padding-left: 50rpx;
    border-radius: 40rpx;
}
button{
    margin-top: 80rpx;
    width: 340rpx;
    border-radius: 30rpx;
    background: linear-gradient(to bottom right, rgb(168, 219, 248), rgb(102, 231, 156));
}
